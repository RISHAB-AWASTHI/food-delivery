  if (event.key === 'Enter') {
    const searchbox = document.getElementById("search-input").value.toUpperCase();
    const storeitems = document.getElementById("products-container");
    const products = document.getElementsByClassName("col-lg-3 col-md-4 col-sm-6 col-12");
    const pname = storeitems.getElementsByTagName("h5");

    let scrollToElement = null;

    for (var i = 0; i < pname.length; i++) {
      let match = products[i].getElementsByTagName('h5')[0];
      if (match) {
        let textValue = match.textContent || match.innerHTML;
        if (textValue.toUpperCase().indexOf(searchbox) > -1) {
          products[i].style.display = "";
          scrollToElement = products[i];
        } else {
          products[i].style.display = "none";
        }
      }
    }

    if (scrollToElement) {
      scrollToElement.scrollIntoView({ behavior: 'smooth', block: 'start' });
    }

    // Prevent the default form submission behavior
    event.preventDefault();
  }
};

