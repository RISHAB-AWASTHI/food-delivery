    function toggleContent(targetDiv) {
      const cartContainer = document.getElementById('cart-container');
      const otherDiv = document.getElementById('other-div');
      const additionalDiv = document.getElementById('additional-div');

      if (targetDiv === 'cart-container') {
        cartContainer.style.display = 'block';
        otherDiv.style.display = 'none';
        additionalDiv.style.display = 'none';
        history.pushState({ state: 'cart' }, 'Your Cart', '?cart');
      } else if (targetDiv === 'additional-div') {
        cartContainer.style.display = 'none';
        otherDiv.style.display = 'none';
        additionalDiv.style.display = 'block';
        history.pushState({ state: 'additional' }, 'Additional Div', '?additional');
      }
    }

    window.addEventListener('popstate', function (event) {
      const state = event.state;
      const cartContainer = document.getElementById('cart-container');
      const otherDiv = document.getElementById('other-div');
      const additionalDiv = document.getElementById('additional-div');

      if (state === 'cart') {
        cartContainer.style.display = 'block';
        otherDiv.style.display = 'none';
        additionalDiv.style.display = 'none';
      } else if (state === 'additional') {
        cartContainer.style.display = 'none';
        otherDiv.style.display = 'none';
        additionalDiv.style.display = 'block';
      } else {
        cartContainer.style.display = 'none';
        otherDiv.style.display = 'block';
        additionalDiv.style.display = 'none';
      }
    });
  </script>


