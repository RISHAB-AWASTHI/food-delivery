      <p><strong>Email:</strong> ${email}</p>
      <p><strong>Address:</strong> ${address}</p>
      <!-- Add more fields as needed -->

      <hr class="mb-4">
      <p><strong>Payment Method:</strong> ${getSelectedPaymentMethod()}</p>
      <!-- Add more payment-related information as needed -->
    `;

    // Display the checkout information
    const checkoutInfoDiv = document.getElementById('checkout-info');
    checkoutInfoDiv.innerHTML = checkoutInfoHTML;
    checkoutInfoDiv.style.display = 'block';

    // Hide the form
    document.querySelector('form').style.display = 'none';
  }

  function getSelectedPaymentMethod() {
    // Get the selected payment method
    const paymentMethodInputs = document.getElementsByName('paymentMethod');
    const selectedPaymentMethod = Array.from(paymentMethodInputs).find(input => input.checked);

    return selectedPaymentMethod ? selectedPaymentMethod.nextSibling.textContent.trim() : 'N/A';
  }
</script>

</body>

