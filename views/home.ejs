  <script>
    let quantities = {}; 
    let cartItems = []; 

    // Function to increment quantity for a specific product
    function incrementQuantity(productID) {
      if (!quantities[productID]) {
        quantities[productID] = 1;
      } else {
        quantities[productID]++;
      }
      updateQuantity(productID);
    }

    // Function to decrement quantity for a specific product
    function decrementQuantity(productID) {
      if (quantities[productID] && quantities[productID] > 0) {
        quantities[productID]--;
        updateQuantity(productID);
      }
    }

    // Function to update the quantity display for a specific product
    function updateQuantity(productID) {
      setTimeout(() => {
        const quantitySpan = document.getElementById(`quantity_${productID}`);
        if (quantitySpan) {
          quantitySpan.innerText = quantities[productID];
        }
      }, 0);
    }

    // Function to add items to the cart
    function addToCart(itemName, price, productID) {
      
      const existingItemIndex = cartItems.findIndex(item => item.name === itemName);

      if (existingItemIndex !== -1) {
        // If the item already exists in the cart, update the quantity
        cartItems[existingItemIndex].quantity = quantities[productID];
      } else {
        // If the item doesn't exist in the cart, add it
        cartItems.push({
          name: itemName,
          price: price,
          quantity: quantities[productID],
          // Add more properties if needed
        });
      }

      // Update the cart display
      updateCartDisplay();
    }

    // Function to update the cart display
    function updateCartDisplay() {
      // Display a simple alert with the current items in the cart
      const cartItemNames = cartItems.map(item => `${item.name} x${item.quantity}`).join('\n');
      alert(`Cart Items:\n${cartItemNames}`);
    }

  </script>

<script>
  function updateCartDisplay() {
    const cartList = document.getElementById('cart-list');
    const totalPriceElement = document.getElementById('total-price');
    const totalItemsElement = document.getElementById('total-items');

    // Initialize total price and total items
    let totalPrice = 0;
    let totalItems = 0;

    // Clear the existing content of the cartList
    cartList.innerHTML = '';

    // Check if cartItems is empty
    if (cartItems.length === 0) {
      // Display "Cart is empty" message
      const emptyCartMessage = document.createElement('tr');
      emptyCartMessage.innerHTML = '<td colspan="4">Cart is empty</td>';
      cartList.appendChild(emptyCartMessage);
    } else {
      // Iterate through cartItems and update the cart display
      for (const item of cartItems) {
        // Skip items with quantity 0
        if (item.quantity === 0) {
          continue;
        }

        const listItem = document.createElement('tr');
        const itemTotalPrice = item.price * item.quantity;

        // Apply styling to the cart item
        listItem.innerHTML = `
          <td>${item.name}</td>
          <td>${item.quantity}</td>
          <td>Rs ${item.price.toFixed(2)}</td>
          <td>Rs ${itemTotalPrice.toFixed(2)}</td>
        `;
        cartList.appendChild(listItem);

        // Update total price and total items
        totalPrice += itemTotalPrice;
        totalItems += item.quantity;
      }
    }

    // Display total items and total price in the cart
    totalItemsElement.textContent = `Total Items: ${totalItems}`;
    totalPriceElement.textContent = `Total Price: Rs ${totalPrice.toFixed(2)}`;

    // Display total price in the checkout section
    const checkoutTotalPriceElement = document.getElementById('checkout-total-price');
    if (checkoutTotalPriceElement) {
      checkoutTotalPriceElement.textContent = `Total Price: Rs ${totalPrice.toFixed(2)}`;
    }

    // Debugging: Log the values
    console.log('Total Items:', totalItems);
    console.log('Total Price:', totalPrice);
  }

  // Call updateCartDisplay whenever cartItems is updated
  updateCartDisplay();
</script>


<script>
  const searchFun = (event) => {
